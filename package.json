{
  "name": "qa-api",
  "version": "1.0.0",
  "description": "Questions and Answers Rest API",
  "main": "APP.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "start": "cross-env NODE_ENV=production node dist/APP.js",
    "start:dev": "cross-env NODE_ENV=development node dist/APP.js",
    "start:test": "cross-env NODE_ENV=test node dist/APP.js",
    "start:docker": "cross-env NODE_ENV=docker node dist/APP.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node APP.ts",
    "dev:test": "cross-env NODE_ENV=test nodemon --exec ts-node APP.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "code:check": "npm run lint:check && npm run format:check",
    "code:fix": "npm run lint:fix && npm run format",
    "perf:env": "ts-node performance/start-test-environment.ts",
    "perf:setup": "k6 run performance/setup-test-users.ts",
    "perf:test": "ts-node performance/run-tests.ts",
    "perf:quick": "ts-node performance/run-tests.ts quick",
    "perf:load": "ts-node performance/run-tests.ts load",
    "perf:spike": "ts-node performance/run-tests.ts spike",
    "perf:stress": "ts-node performance/run-tests.ts stress",
    "perf:all": "ts-node performance/run-tests.ts all",
    "perf:i18n": "k6 run performance/tests/i18n-cache-test.ts",
    "analyze": "ts-node performance/analyze-results.ts",
    "analyze:latest": "ts-node performance/analyze-results.ts latest",
    "analyze:list": "ts-node performance/analyze-results.ts list",
    "analyze:compare": "ts-node performance/analyze-results.ts compare",
    "smtp:setup": "node scripts/smtp/setup-smtp.js",
    "smtp:setup:dev": "node scripts/smtp/setup-smtp.js dev",
    "smtp:setup:test": "node scripts/smtp/setup-smtp.js test",
    "smtp:setup:prod": "node scripts/smtp/setup-smtp.js prod",
    "smtp:setup:docker": "node scripts/smtp/setup-smtp.js docker",
    "test:websocket": "node scripts/run-tests.js",
    "test:monitoring": "jest tests/unit/services/WebSocketMonitorService.test.ts tests/integration/monitoring.test.ts tests/integration/websocket-client.test.ts --verbose"
  },
  "keywords": [
    "express",
    "rest api"
  ],
  "author": "Mert Baran",
  "license": "MIT",
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "@types/ws": "^8.18.1",
    "amqplib": "^0.10.3",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "google-auth-library": "^10.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "minimist": "^1.2.8",
    "mongoose": "^8.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "pino": "^8.21.0",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "slugify": "^1.6.6",
    "tsyringe": "^4.10.0",
    "ws": "^8.18.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/minimist": "^1.2.5",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.13",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.4",
    "nodemon": "^3.1.9",
    "prettier": "^3.6.2",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
