{
  "name": "qa-api",
  "version": "1.0.0",
  "description": "Questions and Answers Rest API",
  "main": "APP.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "start": "node dist/APP.js",
    "dev": "nodemon --exec ts-node APP.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "perf:setup": "ts-node performance/setup-test-users.ts",
    "perf:basic": "k6 run performance/tests/basic-load-test.ts",
    "perf:stress": "k6 run performance/tests/stress-test.ts",
    "perf:spike": "k6 run performance/tests/spike-test.ts",
    "perf:i18n": "k6 run performance/tests/i18n-cache-test.ts",
    "perf:all": "npm run perf:basic && npm run perf:i18n && npm run perf:stress && npm run perf:spike",
    "metrics:all": "ts-node performance/metrics-reporter.ts all",
    "metrics:basic": "ts-node performance/metrics-reporter.ts basic",
    "metrics:i18n": "ts-node performance/metrics-reporter.ts i18n",
    "metrics:stress": "ts-node performance/metrics-reporter.ts stress",
    "metrics:spike": "ts-node performance/metrics-reporter.ts spike",
    "metrics:quick": "ts-node performance/metrics-reporter.ts quick",
    "view:list": "ts-node performance/view-metrics.ts list",
    "view:latest": "ts-node performance/view-metrics.ts latest",
    "view:compare": "ts-node performance/view-metrics.ts compare"
  },
  "keywords": [
    "express",
    "rest api"
  ],
  "author": "Mert Baran",
  "license": "MIT",
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "google-auth-library": "^10.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "pino": "^8.21.0",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "slugify": "^1.6.6",
    "tsyringe": "^4.10.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.13",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.4",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
